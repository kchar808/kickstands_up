@stations.map do |station|
  {
    name: station.name,
    street_address: station.street_address,
    city: station.city,
    state: station.state,
    brand: station.brand,
    octane: station.octane,
    phone_number: station.phone_number,
    position: {lat: station.lat, lng: station.lng}
  }
end